  "keywords": [
    "animation",
    "css",
    "javascript",
    "react"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dulcetti/fireworks-react"
  },
  "bugs": {
    "url": "https://github.com/dulcetti/fireworks-react/issues"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
